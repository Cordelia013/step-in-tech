{
  "name": "step-in-tech",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "depot": "https://gitlab.com/cordeliaguims/step-in-tech.git",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:network": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "build:css": "tailwindcss -i ./src/app.css -o ./src/output.css --watch",
    "vercel": "vercel --prod",
    "status": "vercel inspect",
    "remove": "vercel remove",
    "deploy": "vercel --prod --yes"
  },
  "dependencies": {
    "@studio-freight/lenis": "^1.0.42",
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.1",
    "roughjs": "^4.6.6",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
